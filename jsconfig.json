{
  "compilerOptions": {
    "baseUrl": "./src",
    "paths": {
      // Components
      "@/common/*": ["components/common/*"],
      "@/hoc/*": ["components/hoc/*"],
      "@/error/*": ["components/error/*"],

      // Components Qasir UI
      "@/qasir-ui/inputs/*": ["components/DS/inputs/*"],
      "@/qasir-ui/navigation/*": ["components/DS/navigation/*"],
      "@/qasir-ui/data-display/*": ["components/DS/data-display/*"],
      "@/qasir-ui/surfaces/*": ["components/DS/surfaces/*"],
      "@/qasir-ui/ui-kit/*": ["components/DS/ui-kit/*"],

      // Layouts
      "@/layout/*": ["layouts/*"],

      // Config
      "@/apis": ["config/apis"],
      "@/constants": ["config/constants"],

      // Lib
      "@/redux/*": ["lib/redux/*"],

      // Styles
      "@/globalStyle/*": ["styles/global/*"],
      "@/override/*": ["styles/override/*"],

      // Utils
      "@/helpers": ["utils/helpers"],
      "@/tracker": ["utils/tracker"],
      "@/lstorage": ["utils/lstorage"],
      "@/request": ["utils/request"],

      // Node modules
      "@/node-modules": ["../node_modules"]
    }
  }
}
